<templateSet group="Mockito">
  <template name="mock" value="final $CLASS_NAME$ $VARIABLE_NAME$Mock = mock($CLASS_NAME$.class);" description="" toReformat="false" toShortenFQNames="true">
    <variable name="CLASS_NAME" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="VARIABLE_NAME" expression="" defaultValue="camelCase(CLASS_NAME)" alwaysStopAt="true" />
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
  <template name="stub" value="final $CLASS_NAME$ $VARIABLE_NAME$Stub = mock($CLASS_NAME$.class);" description="" toReformat="false" toShortenFQNames="true">
    <variable name="CLASS_NAME" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="VARIABLE_NAME" expression="" defaultValue="camelCase(CLASS_NAME)" alwaysStopAt="true" />
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
</templateSet>